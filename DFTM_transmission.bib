issn = {1049-9644},
journal = {Biological Control},
keywords = {AcMNPV-hsp70/lacZ,M nucleopolyhedrovirus,M2R,optical brighteners,viral pathogenesis.,$\beta$-galactosidase},
number = {1},
pages = {58--69},
title = {{Evidence that the stilbene-derived optical brightener M2R enhances \textit{Autographa californica} M nucleopolyhedrovirus infection of \textit{Trichoplusia ni} and \textit{Heliothis virescens} by preventing sloughing of infected midgut epithelial cells}},
