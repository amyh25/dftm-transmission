pages = {295--300},
pmid = {11369107},
publisher = {Elsevier},
title = {{How should pathogen transmission be modelled?}},
url = {http://www.cell.com/article/S0169534701021449/fulltext http://www.cell.com/article/S0169534701021449/abstract https://www.cell.com/trends/ecology-evolution/abstract/S0169-5347(01)02144-9},
volume = {16},
