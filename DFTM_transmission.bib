number = {3},
pages = {158--165},
title = {{Influence of the host plant on occluded virus production and lethal infectivity of a baculovirus}},
url = {http://www.sciencedirect.com/science/article/pii/S0022201102001933},
volume = {81},
year = {2002}
}
