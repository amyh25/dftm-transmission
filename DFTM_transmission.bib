@article{BruijningEtAl2020,
abstract = {Genetically identical individuals can be phenotypically variable, even in constant environmental conditions. The ubiquity of this phenomenon, known as ‘intra-genotypic variability', is increasingly evident and the relevant mechanistic underpinnings are beginning to be understood. In parallel, theory has delineated a number of formal expectations for contexts in which such a feature would be adaptive. Here, we review empirical evidence across biological systems and theoretical expectations, including nonlinear averaging and bet hedging. We synthesize existing results to illustrate the dependence of selection outcomes both on trait characteristics, features of environmental variability, and species' demographic context. We conclude by discussing ways to bridge the gap between empirical evidence of intra-genotypic variability, studies demonstrating its genetic component, and evidence that it is adaptive.},
author = {Bruijning, Marjolein and Metcalf, C. Jessica E. and Jongejans, Eelke and Ayroles, Julien F.},
doi = {10.1016/j.tree.2019.08.005},
file = {:Users/arifreedman/Library/Application Support/Mendeley Desktop/Downloaded/Bruijning et al. - 2020 - The Evolution of Variance Control.pdf:pdf},
issn = {01695347},
journal = {Trends in Ecology and Evolution},
keywords = {bet hedging,intra-genotypic variability,micro-environmental plasticity,nonlinear averaging,phenotypic variance,vQTL},
month = {jan},
number = {1},
pages = {22--33},
pmid = {31519463},
publisher = {Elsevier Ltd},
title = {{The Evolution of Variance Control}},
volume = {35},
year = {2020}
}
